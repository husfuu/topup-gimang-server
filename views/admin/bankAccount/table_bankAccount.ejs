<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>No.</th>
        <th>Nama pemilik</th>
        <th>Nama bank</th>
        <th>Nomor rekening</th>
        <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
    <% for(let i=0; i < bankAccounts.length; i++){ %>
        <tr>
        <td>
            <%= i + 1 %>
        </td>
        <td>
            <%= bankAccounts[i].name %>
        </td>
        <td>
            <%= bankAccounts[i].Admin.email %>
        </td>
        <td>
            <%= bankAccounts[i].accountNumber %>
        </td>
        <td>
            <form action="/bankaccounts/delete/<%= bankAccounts[i].id %>" method="POST">
            <a href="/bank/edit/<%= bankAccounts[i].id %>" class="btn btn-info btn-sm">
                <i class="fas fa-pencil-alt"></i>
                Ubah
            </a>
            <button class="btn btn-danger btn-sm">
                <i class="fas fa-trash"></i>
                Hapus
            </button>
            </form>
        </td>
        </tr>
        <% } %>
    </tbody>
</table>